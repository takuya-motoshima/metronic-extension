    <div class="demonstration-block">
      <div class="py-10">
        <div id="softLimitsSlider"></div>
      </div>
    </div>
  </div>
  <!--end::Block-->
  <!--begin::Code-->
  <div class="py-5">
    <!--begin::Highlight-->
    <div class="highlight">
      <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
      <ul class="nav nav-pills" role="tablist">
        <li class="nav-item"><a class="nav-link active" data-bs-toggle="tab" href="#soft-limits-slider-html-tab" role="tab">HTML</a></li>
        <li class="nav-item"><a class="nav-link" data-bs-toggle="tab" href="#soft-limits-slider-js-tab" role="tab">JAVASCRIPT</a></li>
      </ul>
      <!--begin::Tab content-->
      <div class="tab-content">
        <!--begin::Tab panel-->
        <div class="tab-pane fade show active" id="soft-limits-slider-html-tab" role="tabpanel">
          <div class="highlight-code">
            <pre class="language-html">
              <code class="language-html">&lt;div id=&quot;softLimitsSlider&quot;&gt;&lt;/div&gt;</code>
            </pre>
          </div>
        </div>
        <!--end::Tab panel-->
        <!--begin::Tab panel-->
        <div class="tab-pane fade" id="soft-limits-slider-js-tab" role="tabpanel">
          <div class="highlight-code">
            <pre class="language-javascript">
              <code class="language-javascript">// Initialize soft limits slider.
const slider = document.getElementById(&#039;softLimitsSlider&#039;);

noUiSlider.create(slider, {
  start: [50],
  range: {
    min: 0,
    max: 100
  },
  pips: {
    mode: &#039;values&#039;,
    values: [20, 80],
    density: 4
  }
});</code>
            </pre>
          </div>
        </div>
        <!--end::Tab panel-->
