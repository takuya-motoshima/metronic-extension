<!--begin::Section-->
<div class="pb-10">
  <!--begin::Heading-->
  <h1 class="anchor fw-bold mb-5" id="overview" data-kt-scroll-offset="100">
    <a href="#overview"></a>Overview
  </h1>
  <!--end::Heading-->
  <!--begin::Block-->
  <div class="my-5">
    noUiSlider is a lightweight range slider with multi-touch support and a ton of features. It supports non-linear ranges, 
    requires no external dependencies, has keyboard support, and it works great in responsive designs.
    For full documentation please check the <a href="https://refreshless.com/nouislider" target="_blank">plugin's site</a>.
  </div>
  <!--end::Block-->
</div>
<!--end::Section-->
<!--begin::Section-->
<div class="pb-10">
  <!--begin::Heading-->
  <h1 class="anchor fw-bold mb-5" id="basic" data-kt-scroll-offset="100">
    <a href="#basic"></a>Basic
  </h1>
  <!--end::Heading-->
  <!--begin::Block-->
  <div class="my-5">
    Basic example of range slider with full touch support.
  </div>
  <!--end::Block-->
  <!--begin::Block-->
  <div class="py-5">
    <div class="demonstration-block">
      <div class="mb-0">
        <label class="form-label">Example</label>
        <div id="basicSlider"></div>
        <div class="pt-5">
          <div class="fw-semibold mb-2">Min: <span id="basicSliderMin"></span></div>
          <div class="fw-semibold mb-2">Max: <span id="basicSliderMax"></span></div>
        </div>
      </div>
    </div>
  </div>
  <!--end::Block-->
  <!--begin::Code-->
  <div class="py-5">
    <!--begin::Highlight-->
    <div class="highlight">
      <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
      <ul class="nav nav-pills" role="tablist">
        <li class="nav-item"><a class="nav-link active" data-bs-toggle="tab" href="#basic-slider-html-tab" role="tab">HTML</a></li>
        <li class="nav-item"><a class="nav-link" data-bs-toggle="tab" href="#basic-slider-js-tab" role="tab">JAVASCRIPT</a></li>
      </ul>
      <!--begin::Tab content-->
      <div class="tab-content">
        <!--begin::Tab panel-->
        <div class="tab-pane fade show active" id="basic-slider-html-tab" role="tabpanel">
          <div class="highlight-code">
            <pre class="language-html">
              <code class="language-html">&lt;div class=&quot;mb-0&quot;&gt;
  &lt;label class=&quot;form-label&quot;&gt;Example&lt;/label&gt;
  &lt;div id=&quot;basicSlider&quot;&gt;&lt;/div&gt;
  &lt;div class=&quot;pt-5&quot;&gt;
    &lt;div class=&quot;fw-semibold mb-2&quot;&gt;Min: &lt;span id=&quot;basicSliderMin&quot;&gt;&lt;/span&gt;&lt;/div&gt;
    &lt;div class=&quot;fw-semibold mb-2&quot;&gt;Max: &lt;span id=&quot;basicSliderMax&quot;&gt;&lt;/span&gt;&lt;/div&gt;
  &lt;/div&gt;
&lt;/div&gt;</code>
            </pre>
          </div>
        </div>
        <!--end::Tab panel-->
        <!--begin::Tab panel-->
        <div class="tab-pane fade" id="basic-slider-js-tab" role="tabpanel">
          <div class="highlight-code">
            <pre class="language-javascript">
              <code class="language-javascript">// Initialize basic slider.
const slider = document.getElementById(&#039;basicSlider&#039;);
const valueMin = document.getElementById(&#039;basicSliderMin&#039;);
const valueMax = document.getElementById(&#039;basicSliderMax&#039;);

noUiSlider.create(slider, {
  start: [20, 80],
  connect: true,
  range: {
    min: 0,
    max: 100
  }
});

slider.noUiSlider.on(&#039;update&#039;, (values, handle) =&gt; {
  if (handle)
    valueMax.innerHTML = values[handle];
  else
    valueMin.innerHTML = values[handle];
});</code>
            </pre>
          </div>
        </div>
        <!--end::Tab panel-->
      </div>
      <!--end::Tab content-->
    </div>
    <!--end::Highlight-->
  </div>
  <!--end::Code-->
</div>
<!--end::Section-->
<!--begin::Section-->
<div class="pb-10">
  <!--begin::Heading-->
  <h1 class="anchor fw-bold mb-5" id="sizes" data-kt-scroll-offset="100">
    <a href="#sizes"></a>Sizes
  </h1>
  <!--end::Heading-->
  <!--begin::Block-->
  <div class="my-5">
    Change the sliders default size by applying custom size classes <code>.noUi-sm</code> and <code>.noUi-lg</code>.
  </div>
  <!--end::Block-->
  <!--begin::Block-->
  <div class="py-5">
    <div class="demonstration-block">
      <div class="mb-10">
        <label class="form-label">Small size</label>
        <div id="smallSlider" class="noUi-sm"></div>
      </div>
      <div class="mb-10">
        <label class="form-label">Default size</label>
        <div id="normalSizeSlider"></div>
      </div>
      <div class="mb-0">
        <label class="form-label">Large size</label>
        <div id="largeSlider" class="noUi-lg"></div>
      </div>
    </div>
  </div>
  <!--end::Block-->
  <!--begin::Code-->
  <div class="my-5">
    <!--begin::Highlight-->
    <div class="highlight">
      <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
      <div class="highlight-code">
        <pre class="language-html">
          <code class="language-html">&lt;div id=&quot;smallSlider&quot; class=&quot;noUi-sm&quot;&gt;&lt;/div&gt;
&lt;div id=&quot;normalSizeSlider&quot;&gt;&lt;/div&gt;
&lt;div id=&quot;largeSlider&quot; class=&quot;noUi-lg&quot;&gt;&lt;/div&gt;</code>
        </pre>
      </div>
    </div>
    <!--end::Highlight-->
  </div>
  <!--end::Code-->
</div>
<!--end::Section-->
