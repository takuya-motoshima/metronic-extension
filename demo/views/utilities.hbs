              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {isString} from &#039;metronic-extension&#039;;

isString(&#039;a&#039;);// -&gt; true
isString(Object(&#039;a&#039;));// -&gt; true

isString([1, 2, 3]);// -&gt; false
isString(true);// -&gt; false
isString(new Date());// -&gt; false
isString(new Error());// -&gt; false
isString({0: 1, length: 1});// -&gt; false
isString(1);// -&gt; false
isString(/x/);// -&gt; false
isString(Symbol(&#039;a&#039;));// -&gt; false</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>isSymbol()</code>
            </td>
            <td>
              Checks if the type is Symbol.
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>payload: any</code>
                  <!--begin::Block-->
                  <div class="pt-3">The value to test.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
