              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {validators} from &#039;metronic-extension&#039;;

validators.isFQDNorIP(&#039;domain.com&#039;);
validators.isFQDNorIP(&#039;127.0.0.1&#039;);
validators.isFQDNorIP(&#039;127.0.0.1&#039;);
validators.isFQDNorIP(&#039;2001:db8:0000:1:1:1:1:1&#039;);
validators.isFQDNorIP(&#039;1.2.3.4&#039;, {ipVersion: 4});
validators.isFQDNorIP(&#039;::ffff:127.0.0.1&#039;, {ipVersion: 6});</code>
