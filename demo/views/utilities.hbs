        <tbody>
          <tr>
            <td>
              <code>escapeHtml()</code>
            </td>
            <td>
              Escapes HTML special characters (&amp;&#039;`&quot;&lt;&gt;).
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>str: string</code>
                  <!--begin::Block-->
                  <div class="pt-3">String to escape.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Return value:</div>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>string</code> Escaped string.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {escapeHtml} from 'metronic-extension';

escapeHtml('I <b>think</b> this is good.');// -> I <b>think</b> this is good.
escapeHtml('John "Johnny" Smith');// -> John "Johnny" Smith</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>fetchDataUrl()</code>
            </td>
            <td>
              Get the fetched result as a Data URL.
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>url: string|URL</code>
                  <!--begin::Block-->
                  <div class="pt-3">URL string or URL object.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Return value:</div>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>Promise&lt;string&gt;</code> Data URL.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {fetchDataUrl} from &#039;metronic-extension&#039;;

await fetchDataUrl(&#039;/img/sample.png&#039;);// -&gt; data:image/png;base64,iVBORw0K...
await fetchDataUrl(&#039;/img/sample.svg&#039;);// -&gt; data:image/svg+xml;utf8,%3Csvg...</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>fetchImage()</code>
            </td>
            <td>
              Get the fetched result as an Image object.
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>url: string</code>
                  <!--begin::Block-->
                  <div class="pt-3">URL.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Return value:</div>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>Promise&lt;HTMLImageElement&gt;</code> Image object.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {fetchImage} from &#039;metronic-extension&#039;;

await fetchImage(&#039;/img/1.png&#039;);// -&gt; &lt;img crossorigin=&quot;use-credentials&quot; src=&quot;/img/1.png&quot;&gt;</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>formatBytes()</code>
            </td>
            <td>
              Convert bytes to strings with units (KB, MB, GB, etc.).
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>bytes: string|number</code>
                  <!--begin::Block-->
                  <div class="pt-3">Bytes.</div>
                  <!--end::Block-->
                </li>
                <li class="py-2"><code>decimals?: number</code>
                  <!--begin::Block-->
                  <div class="pt-3">Number of decimal places. Default is 2.</div>
                  <!--end::Block-->
                </li>
                <li class="py-2"><code>asString?: boolean</code>
                  <!--begin::Block-->
                  <div class="pt-3">If true, returns the result as a string; if false, returns the result as a value and unit object ({val: number, unit: string}). Default is false.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Return value:</div>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>string|{val: number, unit: string}</code> Value with units.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {formatBytes} from &#039;metronic-extension&#039;;

formatBytes(435, 2, true);// -&gt; 435 Bytes
formatBytes(490398, 2, true);// -&gt; 478.9 KB
formatBytes(23483023, 2, true);// -&gt; 22.4 MB
formatBytes(30498505889, 2, true);// -&gt; 28.4 GB
formatBytes(9485039485039445, 2, true);// -&gt; 8.42 PB
formatBytes(9485039485039445);// -&gt; {val: 8.42, unit: &#039;PB&#039;}</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>getExtensionFromDataUrl()</code>
            </td>
            <td>
              Get extension from Data URL.
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>value: string</code>
                  <!--begin::Block-->
                  <div class="pt-3">Data URL.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Return value:</div>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>string|null</code> Extension. For example, jpeg, svg.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {getExtensionFromDataUrl} from &#039;metronic-extension&#039;;

getExtensionFromDataUrl(&#039;data:text/html,Hello%2C%20World!&#039;);// -&gt; html
getExtensionFromDataUrl(&#039;data:image/jpeg;base64,/9j/4AAQS&#039;);// -&gt; jpeg
getExtensionFromDataUrl(&#039;data:image/png;base64,iVBORw0KGg&#039;);// -&gt; png
getExtensionFromDataUrl(&#039;data:image/svg+xml;base64,PHN2Zy&#039;);// -&gt; svg
getExtensionFromDataUrl(&#039;data:application/pdf;base64,JVBE&#039;);// -&gt; pdf</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>getType()</code>
            </td>
            <td>
              Get type name.
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>payload: any</code>
                  <!--begin::Block-->
                  <div class="pt-3">The value to test.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Return value:</div>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>string</code> Type Name.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {getType} from &#039;metronic-extension&#039;;

getType(3);// -&gt; Number
getType(3.4);// -&gt; Number
getType(NaN);// -&gt; Number
getType(&#039;ram&#039;);// -&gt; String
getType(new String(&#039;ram&#039;));// -&gt; String
getType(new Date());// -&gt; Date
getType(true);// -&gt; Boolean
getType(false);// -&gt; Boolean
getType(new Boolean(true));// -&gt; Boolean
getType({});// -&gt; Object
getType(Object.create(null));// -&gt; Object
getType({name: &#039;Narendra&#039;});// -&gt; Object
getType(new class Person{});// -&gt; Object
getType([]);// -&gt; Array
getType(new Array(3, 4));// -&gt; Array
getType([3, 4]);// -&gt; Array
getType(parseFloat);// -&gt; Function
getType(function(){});// -&gt; Function
getType(Symbol(&#039;foo&#039;));// -&gt; Symbol
getType(null);// -&gt; Null
getType(undefined);// -&gt; Undefined</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>isAsyncFunction()</code>
            </td>
            <td>
              Check if it is an asynchronous function.
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>payload: any</code>
                  <!--begin::Block-->
                  <div class="pt-3">The value to test.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Return value:</div>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>boolean</code> True for asynchronous functions, false otherwise.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {isAsyncFunction} from &#039;metronic-extension&#039;;

isAsyncFunction(async function func() {});// -&gt; true
isAsyncFunction(function func() {});// -&gt; false
isAsyncFunction(true);// -&gt; false
isAsyncFunction(false);// -&gt; false
isAsyncFunction(null);// -&gt; false
isAsyncFunction(undefined);// -&gt; false
isAsyncFunction({});// -&gt; false
isAsyncFunction([]);// -&gt; false
isAsyncFunction(/a/g);// -&gt; false
isAsyncFunction(&#039;string&#039;);// -&gt; false
isAsyncFunction(42);// -&gt; false
isAsyncFunction(new Date());// -&gt; false</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>isEmpty()</code>
            </td>
            <td>
              Check if the value is empty.
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>payload: any</code>
                  <!--begin::Block-->
                  <div class="pt-3">The value to test.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Return value:</div>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>boolean</code> True for blank, null, undefined, NaN, empty array, empty object, empty File, empty Set, or empty Map; false otherwise.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <div class="highlight-code">
                  <pre class="language-javascript">
                    <code class="language-javascript">import {isEmpty} from &#039;metronic-extension&#039;;

isEmpty(&#039;&#039;);// -&gt; true
isEmpty(&#039; &#039;);// -&gt; true
isEmpty(null);// -&gt; true
isEmpty(undefined);// -&gt; true
isEmpty(NaN);// -&gt; true
isEmpty([]);// -&gt; true
isEmpty({});// -&gt; true
isEmpty(new Set());// -&gt; true
isEmpty(new Map());// -&gt; true
isEmpty(new Map());// -&gt; true
isEmpty(new File([&#039;&#039;], &#039;foo.txt&#039;));// -&gt; true

isEmpty(&#039;string&#039;);// -&gt; false
isEmpty([&#039;a&#039;, &#039;b&#039;]);// -&gt; false
isEmpty({a: &#039;b&#039;});// -&gt; false
isEmpty(0);// -&gt; false
isEmpty(42);// -&gt; false
isEmpty(function() {});// -&gt; false
isEmpty(false);// -&gt; false
isEmpty(true);// -&gt; false
isEmpty(new Set([1,2,3]));// -&gt; false
isEmpty(new Map([[&#039;key&#039;, &#039;value&#039;]]));// -&gt; false
isEmpty(new File([&#039;bar&#039;], &#039;bar.txt&#039;));// -&gt; false</code>
                  </pre>
                </div>
              </div>
              <!--end::Highlight-->
            </td>
          </tr>
          <tr>
            <td>
              <code>isFunction()</code>
            </td>
            <td>
              Check if it is a function.
              <!--begin::Heading-->
              <div class="py-3 text-gray-700">Parameters:</div>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>payload: any</code>
